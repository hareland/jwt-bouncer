{
    "name": "josepostiga/jwt-bouncer",
    "description": "A verification package for your JWT authenticated requests.",
    "keywords": ["php", "package", "laravel", "jwt-bouncer", "jwt"],
    "homepage": "https://github.com/josepostiga/jwt-bouncer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Postiga",
            "email": "me@josepostiga.com",
            "homepage": "https://josepostiga.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "lcobucci/jwt": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "php-coveralls/php-coveralls": "^2.2",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "JosePostiga\\JwtBouncer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosePostiga\\JwtBouncer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JosePostiga\\JwtBouncer\\JwtBouncerServiceProvider"
            ]
        }
    }
}
